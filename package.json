{
  "name": "chekpoint",
  "version": "1.0.0",
  "description": "Control de acceso vehicular para puerto comercial con Electron y React.",
  "main": "main.js",
  "scripts": {
    "start": "webpack serve --host 0.0.0.0",
    "react-start": "cd src && npm start",
    "electron-start": "wait-on tcp:3000 && electron .",
    "build": "webpack --mode production",
    "optimize:dark2": "node scripts/resize-dark2.js",
    "package": "electron-packager . chekpoint --platform=win32 --arch=x64  --out=dist --overwrite",
    "test": "jest --runInBand"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^37.2.6",
    "electron-packager": "^17.1.1",
    "electron-reload": "^2.0.0-alpha.1",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "style-loader": "^4.0.0",
    "wait-on": "^7.0.1",
    "webpack": "^5.101.0"
  },
  "dependencies": {
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "html5-qrcode": "^2.3.8",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "xlsx": "^0.18.5"
  },
  "keywords": []
}
